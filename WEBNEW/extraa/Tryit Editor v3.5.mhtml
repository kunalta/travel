@charset "utf-8";

a { color: rgb(66, 133, 244); cursor: pointer; text-decoration: none; }

body { font-family: "Google Sans", arial, sans-serif; font-size: 14px; line-height: 1.4286; margin: 0px; }

.icon { height: 20px; width: 20px; }

.dialog-modal { background-color: rgba(0, 0, 0, 0.2); }

.dialog .dialog-header, .dialog .dialog-header .google-icon { display: flex; align-items: center; }

.dialog .dialog-header { border-bottom: 1px solid rgb(213, 213, 213); color: rgb(117, 117, 117); height: 48px; }

.dialog .dialog-header .google-icon { margin: 0px 12px 0px 24px; }

.dialog .dialog-footer { display: flex; justify-content: space-between; font-family: roboto, arial, sans-serif; font-size: 12px; padding: 24px 40px 22px; }

.dialog .dialog-footer .footer-menu { list-style: none; margin: 0px -16px; padding: 0px; }

.menu-item { display: inline-block; margin: 0px; }

.menu-item .menu-content { color: rgb(117, 117, 117); cursor: pointer; font-weight: inherit; padding: 6px 16px; text-decoration: none; }

.language-selector { cursor: pointer; }

.language-selected { display: flex; align-items: center; }

.chevron { border-color: currentcolor transparent transparent; border-style: solid; border-width: 4px 4px 0px; margin: 0px 0px 0px 8px; color: rgb(68, 68, 68); height: 0px; width: 0px; }

.language-list { box-shadow: rgba(0, 0, 0, 0.14) 0px 8px 10px 1px, rgba(0, 0, 0, 0.12) 0px 3px 14px 2px, rgba(0, 0, 0, 0.2) 0px 5px 5px -3px; transition: opacity 0.1s linear 0s; background-color: rgb(255, 255, 255); bottom: 16px; color: rgb(68, 68, 68); line-height: 1.4; list-style: none; min-width: 66px; opacity: 0; overflow: hidden auto; position: fixed; top: 16px; visibility: hidden; z-index: 2000; }

.language-option { cursor: pointer; height: auto; overflow: hidden; padding: 8px 26px 8px 16px; }

.language-option:hover { background-color: rgb(238, 238, 238); }

.dialog { display: flex; flex-direction: column; }

.dialog-body { flex-grow: 1; }

@media (max-width: 450px) {
  .dialog .dialog-footer { padding: 24px 24px 22px; }
}

.dialog-container { min-height: 100vh; display: flex; align-items: center; }

.dialog-container, .rp-icon-container, .identity-chip { justify-content: center; }

.dialog { background-color: rgb(255, 255, 255); }

.dialog .dialog-content { box-sizing: border-box; flex-grow: 1; overflow: hidden; padding: 36px 40px; }

.title { color: rgb(32, 33, 36); font-size: 24px; font-weight: normal; line-height: 32px; margin: 0px; padding-bottom: 0px; text-align: center; text-indent: -1px; }

.consent-form { color: rgb(32, 33, 36); font-family: Roboto, sans-serif; font-size: 16px; line-height: 24px; padding-top: 16px; }

.consent-form .consent-text { margin: 0px; }

.button-group { display: flex; justify-content: space-between; padding-top: 32px; }

.button-group-high { padding: 56px 0px 12px; }

.button { display: flex; justify-content: center; flex-direction: column; border-radius: 4px; cursor: pointer; font-family: Roboto, sans-serif; font-weight: 500; height: 36px; }

.button-confirm { background-color: rgb(66, 133, 244); color: rgb(255, 255, 255); padding: 0px 24px; }

.button-cancel { background-color: rgb(255, 255, 255); color: rgb(66, 133, 244); margin-left: -8px; padding: 0px 8px; }

@media (max-width: 450px) {
  .dialog .dialog-content { padding: 36px 24px; }
  .dialog-body { min-height: 100vh; }
}

@media (min-width: 451px) {
  .dialog { border-radius: 8px; box-shadow: rgba(0, 0, 0, 0.14) 0px 2px 2px 0px, rgba(0, 0, 0, 0.12) 0px 3px 1px -2px, rgba(0, 0, 0, 0.2) 0px 1px 5px 0px; max-width: 500px; }
  .inflated-dialog { min-height: 500px; }
}
